<?php
require 'dbgp/dbgpclient.php';
$data = file_get_contents(dirname(__FILE__) . '/bug00689.inc');

$commands = array(
	'step_into',
	'breakpoint_set -t line -n 3',
	'run',
	'eval -- dW5kZWZpbmVkX2Z1bmN0aW9uKCk7',
	'stack_get',
	'step_into',
	'detach',
);

dbgpRun( $data, $commands );
?>
